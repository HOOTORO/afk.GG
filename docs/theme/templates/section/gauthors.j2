{% if git_info %}
  <div class="authors labeled">
    Authors
  </div>
  {%- for author in git_info.get('page_authors') -%}
 
 
    <div>
      <code>
        {{ author.contribution }} <|
        {% if author.name.startswith("Not") %}
          Community
        {% else %}
          {{ author.name }}
        {% endif %}
        {#At: {{ author.last_date }} ðŸž§
          <a href="{{ author.email }}" title="{{ author.name }}" />,- {{ author.lines }} #}
      </code>
    </div>
  {%- endfor -%}
{% endif %}
