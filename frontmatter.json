{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.content.publicFolder": "",
  "frontMatter.framework.id": "other",
  "frontMatter.framework.startCommand": "./.venv/bin/mkdocs serve --watch-theme",
  "frontMatter.preview.host": "http://localhost:8000",
  "frontMatter.content.pageFolders": [
    {
      "path": "[[workspace]]/docs/src/md/blog",
      "title": "blog"
    },
    {
      "path": "[[workspace]]/docs/src/md",
      "title": "md"
    }
  ],
  "frontMatter.data.files": [
    {
      "file": "[[workspace]]/docs/src/md/blog/authors.yml",
      "fileType": "yaml",
      "id": "authors",
      "title": "Authors",
      "schema": {
        "title": "Author",
        "type": "object",
        "properties": {
          "avatar": {
            "description": "The avatar of the vegetable.",
            "type": "string"
          },
          "description": {
            "description": "The description of the vegetable.",
            "type": "string"
          },
          "name": {
            "description": "The name of the vegetable.",
            "type": "string"
          }
        }
      },
      "singleEntry": false
    }
  ],
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "default",
      "fields": [
        {
          "name": "template",
          "title": "template",
          "type": "string",
          "hidden": true,
          "single": true
        },
        {
          "name": "title",
          "title": "title",
          "type": "string",
          "single": true
        },
        {
          "default": "You should never underestimate the predictability of stupidity",
          "name": "description",
          "title": "description",
          "type": "string",
          "single": true,
          "wysiwyg": false
        },
        {
          "name": "date",
          "title": "date",
          "type": "fields",
          "fields": [
            {
              "dateFormat": "yyyy-MM-dd",
              "default": "{{now}}",
              "name": "created",
              "title": "Created",
              "type": "datetime",
              "isPublishDate": true
            },
            {
              "dateFormat": "yyyy-MM-dd HH:mm",
              "default": "{{now}}",
              "name": "updated",
              "title": "Updated",
              "type": "datetime",
              "isModifiedDate": true
            }
          ]
        },
        {
          "name": "preview",
          "title": "Article preview",
          "type": "image",
          "isPreviewImage": true
        },
        {
          "dataFileId": "authors",
          "dataFileKey": "name",
          "dataFileValue": "name",
          "name": "authors",
          "title": "Authors",
          "type": "dataFile",
          "multiple": true,
          "singleValueAsString": true
        },
        {
          "name": "categories",
          "title": "categories",
          "type": "categories"
        },
        {
          "name": "tags",
          "title": "tags",
          "type": "tags"
        },
        {
          "name": "draft",
          "title": "draft",
          "type": "draft",
          "default": true
        },
        {
          "name": "comments",
          "title": "comments",
          "type": "boolean"
        },
        {
          "name": "hide",
          "title": "hide",
          "type": "choice",
          "choices": [
            "feedback",
            "footer",
            "navigation",
            "toc"
          ],
          "multiple": true,
          "singleValueAsString": false
        },
        {
          "name": "status",
          "title": "status",
          "type": "choice",
          "choices": [
            "new",
            "old"
          ]
        },
        {
          "default": "{{slug}}",
          "name": "slug",
          "title": "Slug",
          "type": "slug",
          "editable": true
        },
        {
          "name": "lastmod",
          "title": "lastmod",
          "type": "datetime",
          "dateFormat": "yyyy-MM-dd HH:mm"
        }
      ],
      "pageBundle": false,
      "previewPath": null
    }
  ],
  "frontMatter.content.autoUpdateDate": true,
  "frontMatter.website.host": "https://hooto.ru",
  "frontMatter.git.enabled": true
}