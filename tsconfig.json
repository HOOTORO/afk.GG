{
  "compilerOptions": {
    "jsx": "react",
    "module": "ESNext",
    "moduleResolution": "Node",
    "noImplicitAny": true,
    "strictNullChecks": false,
    "removeComments": true,
    "rootDir": "src",
    "outDir": "docs/theme/js/ts",
    "target": "ESNext",
    "allowSyntheticDefaultImports": true
  },
  "include": ["src/**/*"]
}
