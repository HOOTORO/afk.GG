{
  "compilerOptions": {
    "esModuleInterop": true,
    // "listFiles": true,
    "jsx": "react",
    "lib": ["DOM", "DOM.Iterable", "ES2023"],
    "module": "ES2022",
    "moduleResolution": "Node",
    "declaration": true,
    "declarationMap": true,
    "inlineSourceMap": true,
    "inlineSources": true,
    "noImplicitAny": true,
    "strictNullChecks": false,
    "removeComments": true,
    "rootDir": "src",
    "outDir": "docs/theme/js/ts",
    "target": "ESNext",
    "plugins": [
      {
        "name": "typescript-eslint-language-service"
      }
    ]
  },
  "include": ["src/**/*"]
}
