{
  "name": "afk.gg",
  "version": "1.0",
  "type": "module",
  "keywords": [
    "AFK Arena",
    "Lilith games",
    "Ru Community"
  ],
  "author": {
    "name": "HOOTORO"
  },
  "scripts": {
    "RE:instal dependencies": "pip install -r requirements.txt",
    "build": "tsc --build",
    "lint.fix": "npx stylelint docs/src/scss/**/*.scss",
    "SASS:build": "sass docs/src/scss:docs/theme/css/",
    "SASS:watch": "sass --watch docs/src/scss/:docs/theme/css/",
    "RE:build": "tsc --build && npm run -s SASS:build && mkdocs build",
    "RE:serve": "mkdocs serve --watch-theme",
    "SERVE.THIS": "tsc --watch & npm run -s RE:serve & npm run --loglevel=silent SASS:watch ",
    "GHP:Manual Deploy": "mkdocs gh-deploy --force"
  },
  "engines": {
    "node": ">=20.0.0",
    "vscode": "^1.90.0",
    "npm": "~10.0.0"
  },
  "devDependencies": {
    "@mdi/svg": "^7.4.47",
    "@types/cors": "^2.8.13",
    "@types/jquery": "^3.5.16",
    "@types/node": "^20.3.1",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/sqlite3": "^3.1.8",
    "concurrently": "^8.2.0",
    "postcss-inline-svg": "^6.0.0",
    "postcss-pseudo-classes": "^0.4.0",
    "postcss-scss": "^4.0.9",
    "postcss-svgo": "^7.0.1",
    "stylelint": "^16.6.1",
    "stylelint-config-standard-scss": "^13.1.0",
    "stylelint-scss": "*",
    "typescript": "^5.4.5",
    "typescript-eslint-language-service": "^5.0.5"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "glightbox": "^3.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.52.1",
    "sqlite3": "^5.1.6"
  }
}
