{
  "name": "afk.gg",
  "version": "0.5",
  "type": "module",
  "keywords": [
    "AFK Arena",
    "Lilith games",
    "Ru Community"
  ],
  "author": {
    "name": "HOLYDOOM HOOTSMAN"
  },
  "scripts": {
    "SASS:build": "sass --no-source-map docs/src/scss/main.scss docs/theme/css/main.css",
    "SASS:watch": "sass --watch docs/src/scss/main.scss docs/theme/css/main.css",
    "RE:build": "tsc --build && npm run -s SASS:build && mkdocs build",
    "RE:serve": "mkdocs serve --watch-theme",
    "SERVE.THIS": "npm run -s RE:build && tsc --watch & npm run -s SASS:watch & npm run -s RE:serve",
    "GHP:Manual Deploy": "npm run -s RE:build && mkdocs gh-deploy --force"
  },
  "devDependencies": {
    "@types/jquery": "^3.5.16",
    "@types/node": "^20.3.1",
    "concurrently": "^8.2.0",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "glightbox": "^3.2.0"
  }
}
