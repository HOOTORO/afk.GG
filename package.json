{
  "name": "afk.GG",
  "version": "0.5",
  "type": "module",
  "keywords": [
    "AFK Arena",
    "Lilith games",
    "Ru Community"
  ],
  "author": {
    "name": "HOLYDOOM HOOTSMAN"
  },
  "scripts": {
    "SASS:build": "sass --no-source-map scss/main.scss docs/stylesheets/main.css",
    "RE:build": "tsc --build && mkdocs build",
    "serve": "tsc --build && mkdocs serve",
    "watch:scss": "sass --watch scss/extra.scss docs/stylesheets/main.css",
    "serve.th": "tsc --build && tsc --watch & sass --no-source-map scss/main.scss docs/stylesheets/main.css && sass --watch scss/main.scss docs/stylesheets/main.css & mkdocs serve --watch-theme"
  },
  "devDependencies": {
    "@types/jquery": "^3.5.16",
    "@types/node": "^20.3.1",
    "concurrently": "^8.2.0",
    "typescript": "^5.1.3"
  }
}
